@article{ WOS:000489193000006,
Author = {Ghafari, Mahan and Weissman, Daniel B.},
Title = {The expected time to cross extended fitness plateaus},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2019},
Volume = {129},
Number = {SI},
Pages = {54-67},
Month = {OCT},
Note = {Symposium on Feldmania II in honor of Marcus Feldman's 75th birthday,
   Stanford Univ, Stanford, CA, NOV, 2017},
Abstract = {For a population to acquire a complex adaptation requiring multiple
   individually neutral mutations, it must cross a plateau in the fitness
   landscape. We consider plateaus involving three mutations, and show that
   large populations can cross them rapidly via lineages that acquire
   multiple mutations while remaining at low frequency, much faster than
   the proportional to mu(3) rate for simultaneous triple mutations.
   Plateau-crossing is fastest for very large populations. At intermediate
   population sizes, recombination can greatly accelerate adaptation by
   combining independent mutant lineages to form triple-mutants. For more
   frequent recombination, such that the population is kept near linkage
   equilibrium, we extend our analysis to find simple expressions for the
   expected time to cross plateaus of arbitrary width. (C) 2019 Elsevier
   Inc. All rights reserved.},
