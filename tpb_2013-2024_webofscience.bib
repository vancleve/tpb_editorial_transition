@article{ WOS:000366147900004,
Author = {Hu, Linchao and Huang, Mugen and Tang, Moxun and Yu, Jianshe and Zheng,
   Bo},
Title = {<i>Wolbachia</i> spread dynamics in stochastic environments},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2015},
Volume = {106},
Pages = {32-44},
Month = {DEC},
Abstract = {Dengue fever is a mosquito-borne viral disease with 100 million people
   infected annually. A novel strategy for dengue control uses the
   bacterium Wolbachia to invade dengue vector Aedes mosquitoes. As the
   impact of environmental heterogeneity on Wolbachia spread dynamics in
