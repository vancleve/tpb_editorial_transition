@article{ WOS:000322688800004,
Author = {Pokalyuk, Cornelia and Pfaffelhuber, Peter},
Title = {The ancestral selection graph under strong directional selection},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2013},
Volume = {87},
Pages = {25-33},
Month = {AUG},
Abstract = {The ancestral selection graph (ASG) was introduced by Neuhauser and
   Krone (1997) in order to study populations of constant size which evolve
   under selection. Coalescence events, which occur at rate 1 for every
   pair of lines, lead to joint ancestry. In addition, splitting events in
   the ASG at rate alpha, the scaled selection coefficient, produce
   possible ancestors, such that the real ancestor depends on the ancestral
   alleles. Here, we use the ASG in the case without mutation in order to
   study fixation of a beneficial mutant. Using our main tool, a
   reversibility property of the ASG, we provide a new proof of the fact
   that a beneficial allele fixes roughly in time (2 log alpha)/alpha if
   alpha is large. (c) 2012 Elsevier Inc. All rights reserved.},
