@article{ WOS:001159095000001,
Author = {Pina, Veronica Miro and Joly, Emilien and Siri-Jegousse, Arno},
Title = {Estimating the Lambda measure in multiple-merger coalescents},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2023},
Volume = {154},
Pages = {94-101},
Month = {DEC},
Abstract = {Multiple-merger coalescents, also known as A-coalescents, have been used
   to describe the genealogy of popu lations that have a skewed offspring
   distribution or that undergo strong selection. Inferring the
   characteristic measure A, which describes the rates of the
   multiple-merger events, is key to understand these processes. So far,
   most inference methods only work for some particular families of A
   coalescents that are described by only one parameter, but not for more
   general models. This article is devoted to the construction of a
   non-parametric estimator of the density of A that is based on the
   observation at a single time of the so-called Site Frequency Spectrum
   (SPS), which describes the allelic frequencies in a present population
   sample. First, we produce estimates of the multiple-merger rates by
   solving a linear system, whose coefficients are obtained by
   appropriately subsampling the SFS. Then, we use a technique that
   aggregates the information extracted from the previous step through a
   kernel type of re construction to give a non parametric estimation of
   the measure A. We give a consistency result of this estimator under mild
   conditions on the behavior of A around 0. We also show some numerical
   examples of how our method performs,},
