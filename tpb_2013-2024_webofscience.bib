@article{ WOS:000510527700002,
Author = {Huss, Elisabeth and Pfaffelhuber, Peter},
Title = {Genealogical distances under low levels of selection},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2020},
Volume = {131},
Pages = {2-11},
Month = {FEB},
Abstract = {For a panmictic population of constant size evolving under neutrality,
   Kingman's coalescent describes the genealogy of a population sample in
   equilibrium. However, for genealogical trees under selection, not even
   expectations for most basic quantities like height and length of the
   resulting random tree are known. Here, we give an analytic expression
   for the distribution of the total tree length of a sample of size n
   under low levels of selection in a two-alleles model. We can prove that
   trees are shorter than under neutrality under genic selection and if the
   beneficial mutant has dominance h < 1/2, but longer for h > 1/2. The
   difference from neutrality is O(alpha(2)) for genic selection with
   selection intensity alpha and O(alpha) for other modes of dominance. (C)
   2019 Elsevier Inc. All rights reserved.},
