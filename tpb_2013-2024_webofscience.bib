@article{ WOS:000522142000002,
Author = {Martini, Johannes W. R. and Toledo, Fernando H. and Crossa, Jose},
Title = {On the approximation of interaction effect models by Hadamard powers of
   the additive genomic relationship},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2020},
Volume = {132},
Pages = {16-23},
Month = {APR},
Abstract = {Whole genome epistasis models with interactions between different loci
   can be approximated by genomic relationship models based on Hadamard
   powers of the additive genomic relationship. We illustrate that the
   quality of this approximation reduces when the degree of interaction d
   increases. Moreover, considering relationship models defined as weighted
   sum of interactions of different degree, we investigate the impact of
   this decreasing quality of approximation of the summands on the
   approximation of the weighted sum. Our results indicate that these
   approximations remain on a reliable level, but their quality reduces
   when the weights of interactions of higher degrees do not decrease
   quickly. (C) 2020 The Authors. Published by Elsevier Inc.},
