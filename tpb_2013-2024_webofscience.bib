@article{ WOS:000433271000001,
Author = {Ruiz-Herrera, Alfonso},
Title = {Metapopulation dynamics and total biomass: Understanding the effects of
   diffusion in complex networks},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2018},
Volume = {121},
Pages = {1-11},
Month = {MAY},
Abstract = {In this study, I explored the impact of constructing a new dispersal
   route between two different patches in a metapopulation. My results
   indicated that its success failure on the population abundance greatly
   depends on the patches directly involved and negligibly on the network
   topology. Specifically, constructing a dispersal route is highly
   recommended if it connects a source to a source that is close to
   becoming a sink or a sink that is close to becoming a source. This
   biological property is the basis for understanding the influence of the
   network topology on the population abundance. According to some
   thresholds discussed in this manuscript, I identified when a given route
   has a positive or negative effect on the population size. Consequently,
   as a simple rule of thumb, managers should look for metapopulations that
   have the maximum (resp. minimum) number paths with a positive (resp.
   negative) effect on the population abundance. As emphasized, the
   biological results of this paper do not depend on the model formulation.
   (C) 2018 Elsevier Inc. All rights reserved.},
