@article{ WOS:001316088200001,
Author = {Casanova, Adrian Gonzalez and Kurt, Noemi and Perez, Jose Luis},
Title = {The ancestral selection graph for a A-asymmetric Moran model},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2024},
Volume = {159},
Pages = {91-107},
Month = {OCT},
Abstract = {Motivated by the question of the impact of selective advantage in
   populations with skewed reproduction mechanisms, we study a Moran model
   with selection. We assume that there are two types of individuals, where
   the reproductive success of one type is larger than the other. The
   higher reproductive success may stem from either more frequent
   reproduction, or from larger numbers of offspring, and is encoded in a
   measure A for each of the two types. A-reproduction here means that a
   whole fraction of the population is replaced at a reproductive event.
   Our approach consists of constructing a A-asymmetric Moran model in
   which individuals of the two populations compete, rather than
   considering a Moran model for each population. Provided the measure are
   ordered stochastically, we can couple them. This allows us to construct
   the central object of this paper, the A-asymmetric - asymmetric
   ancestral selection graph, leading to a pathwise duality of the forward
   in time A-asymmetric Moran model with its ancestral process. We apply
   the ancestral selection graph in order to obtain scaling limits of the
   forward and backward processes, and note that the frequency process
   converges to the solution of an SDE with discontinuous paths. Finally,
   we derive a Griffiths representation for the generator of the SDE and
   use it to find a semi-explicit formula for the probability of fixation
   of the less beneficial of the two types.},
