   Arizona State Univ, Sch Math \& Stat Sci, Tempe, AZ 85287 USA.},
DOI = {10.1016/j.tpb.2013.03.004},
ISSN = {0040-5809},
EISSN = {1096-0325},
Keywords = {Fluctuating selection; Environmental variation; Genetic drift;
   Genealogies; Structured coalescents; Diffusion approximation},
Keywords-Plus = {BALANCING SELECTION; COALESCENT PROCESS; MODELS; HITCHHIKING; EVOLUTION},
