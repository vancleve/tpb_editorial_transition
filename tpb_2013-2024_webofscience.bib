@article{ WOS:001260003400001,
Author = {Wirtz, Johannes and Guindon, Stephane},
Title = {On the connections between the spatial Lambda-Fleming-Viot model and
   other processes for analysing geo-referenced genetic data},
Journal = {THEORETICAL POPULATION BIOLOGY},
Year = {2024},
Volume = {158},
Pages = {139-149},
Month = {AUG},
Abstract = {The introduction of the spatial Lambda-Fleming-Viot model (AV) in
   population genetics was mainly driven by the pioneering work of Alison
   Etheridge, in collaboration with Nick Barton and Amandine V \&
   eacute;ber about ten years ago (Barton et al., 2010; Barton et al.,
   2013). The AV model provides a sound mathematical framework for
   describing the evolution of a population of related individuals along a
   spatial continuum. It alleviates the ``pain in the torus{''}issue with
   Wright and Mal \& eacute;cot's isolation by distance model and is
   sampling consistent, making it a tool of choice for statistical
   inference. Yet, little is known about the potential connections between
   the AV and other stochastic processes generating trees and the spatial
   coordinates along the corresponding lineages. This work focuses on a
   version of the AV whereby lineages move rapidly over small distances.
   Using simulations, we show that the induced AV tree-generating process
   is well approximated by a birth-death model. Our results also indicate
   that Brownian motions modelling the movements of lines of descent along
   birth-death trees do not generally provide a good approximation of the
   AV due to habitat boundaries effects that play an increasingly important
   role in the long run. Accounting for habitat boundaries through
   reflected Brownian motions considerably increases the similarity to the
   AV model however. Finally, we describe efficient algorithms for fast
   simulation of the backward and forward in time versions of the AV model.},
