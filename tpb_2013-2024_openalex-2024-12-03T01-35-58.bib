	date = {2018-01-31},
	date-added = {2024-12-02 20:58:00 -0500},
	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2018.01.003},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Mate Choice; Mutualistic Networks; Life History Trade-offs; Group Selection},
	la = {en},
	pages = {103--113},
	publisher = {Elsevier BV},
	title = {Non-random mating and information theory},
	url = {https://doi.org/10.1016/j.tpb.2018.01.003},
	volume = {120},
