	journal = {Theoretical Population Biology},
	keywords = {SNP; SNP genotyping; RNA Integrity; SNP array},
	la = {en},
	pages = {23--24},
	publisher = {Elsevier BV},
	title = {Donnelly (1983) and the limits of genetic genealogy},
