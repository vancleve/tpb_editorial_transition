	keywords = {Cultural Evolution; Evolutionary dynamics; Population Genetics; Trait},
	la = {en},
	pages = {118--125},
	publisher = {Elsevier BV},
	title = {The evolutionary advantage of cultural memory on heterogeneous contact networks},
	url = {https://doi.org/10.1016/j.tpb.2018.09.006},
