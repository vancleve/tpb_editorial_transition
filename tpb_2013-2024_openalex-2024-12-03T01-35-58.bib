	keywords = {Tumor Evolution; Population genetics; Single-Cell Analysis; Coalescent theory},
	la = {en},
	pages = {43--50},
	publisher = {Elsevier BV},
	title = {Forward and backward evolutionary processes and allele frequency spectrum in a cancer cell population},
	url = {https://doi.org/10.1016/j.tpb.2017.08.006},
