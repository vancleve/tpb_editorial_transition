	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2023.11.001},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Stochastic game; Comparative statics; Symmetric equilibrium; Collective action; Group Selection},
	la = {en},
