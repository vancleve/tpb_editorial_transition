	keywords = {Multiscale Model; Mathematical Modeling; Scaling Limits; Interacting Particle Systems; Percolation Models},
	la = {en},
	pages = {117--129},
	publisher = {Elsevier BV},
	title = {Large effects and the infinitesimal model},
	url = {https://doi.org/10.1016/j.tpb.2024.02.009},
