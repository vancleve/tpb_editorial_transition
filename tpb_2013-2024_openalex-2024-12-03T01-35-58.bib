	keywords = {Linkage disequilibrium; Dominance (genetics); Local adaptation; Population Dynamics},
	la = {en},
	pages = {42--62},
	publisher = {Elsevier BV},
	title = {The consequences of dominance and gene flow for local adaptation and differentiation at two linked loci},
	url = {https://doi.org/10.1016/j.tpb.2014.04.001},
