	journal = {Theoretical Population Biology},
	keywords = {Social learning; Cultural Evolution},
	la = {en},
	pages = {3--19},
	publisher = {Elsevier BV},
	title = {Evolution of learning strategies in temporally and spatially variable environments: A review of theory},
