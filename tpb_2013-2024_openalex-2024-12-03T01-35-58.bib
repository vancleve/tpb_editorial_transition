	date-added = {2024-12-02 20:58:00 -0500},
	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2017.09.005},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Wright; Coalescent Processes; Coalescence (physics)},
	la = {en},
