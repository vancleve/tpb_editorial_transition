	keywords = {Identity by descent; Genome-wide Association Studies; Haplotype Mapping; Genetic Mapping; Identifiability},
	la = {en},
	pages = {22--29},
	publisher = {Elsevier BV},
	title = {Non-identifiability of identity coefficients at biallelic loci},
