	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Approximate Bayesian Computation; Approximation Algorithms; Model Selection; Nonparametric Methods; Parametric model; Probabilistic Models; Statistical inference},
	la = {en},
	pages = {31--42},
