	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Coalescence (physics); Chromosome Duplication; Genetic Mapping},
	la = {en},
	pages = {67--78},
	publisher = {Elsevier BV},
