	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2023.07.001},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Ectopic recombination; Gene conversion; Concerted evolution; Homologous Recombination; Host-Parasite Coevolution; Multiplex Genome Editing; Non-allelic homologous recombination},
	la = {en},
	pages = {69--90},
	publisher = {Elsevier BV},
	title = {The Recombination Hotspot Paradox: Co-evolution between PRDM9 and its target sites},
