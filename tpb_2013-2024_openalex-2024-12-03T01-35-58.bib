	doi = {10.1016/j.tpb.2015.03.005},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Trait; Quantitative Trait Loci; Quantitative Genetics},
	la = {en},
	pages = {85--93},
	publisher = {Elsevier BV},
	title = {Sensitivity of quantitative traits to mutational effects and number of loci},
	url = {https://doi.org/10.1016/j.tpb.2015.03.005},
	volume = {102},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1016/j.tpb.2015.03.005}}

