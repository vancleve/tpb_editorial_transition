	la = {en},
	pages = {133--147},
	publisher = {Elsevier BV},
	title = {The probability of reciprocal monophyly of gene lineages in three and four species},
	url = {https://doi.org/10.1016/j.tpb.2018.04.004},
	volume = {129},
