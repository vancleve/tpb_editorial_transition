	journal = {Theoretical Population Biology},
	keywords = {Social Learning; Adaptive Evolution},
	la = {en},
	pages = {29--35},
	publisher = {Elsevier BV},
	title = {Success-biased social learning in a one-consumer, two-resource model},
	url = {https://doi.org/10.1016/j.tpb.2022.05.004},
	volume = {146},
	year = {2022},
