	doi = {10.1016/j.tpb.2021.03.001},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Divergence (linguistics); Species Delimitation; Genetic algorithm},
	la = {en},
	pages = {1--15},
	publisher = {Elsevier BV},
	title = {Inference of gene flow in the process of speciation: Efficient maximum-likelihood implementation of a generalised isolation-with-migration model},
