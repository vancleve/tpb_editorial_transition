	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Extinction (optical mineralogy); Habitat Fragmentation; Evolution; Conservation Biology; Habitat Suitability; Experimental Evolution; Niche construction},
	la = {en},
