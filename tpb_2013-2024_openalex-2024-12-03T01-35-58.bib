	keywords = {Sexual dimorphism; Population Genetics; Mendelian inheritance; Genetic model},
	la = {en},
	pages = {160--169},
	publisher = {Elsevier BV},
	title = {Selection in two-sex stage-structured populations: Genetics, demography, and polymorphism},
