	doi = {10.1016/j.tpb.2014.10.006},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Linkage disequilibrium; Spatial Patterns},
	la = {en},
	pages = {43--66},
	publisher = {Elsevier BV},
	title = {Clines in quantitative traits: The role of migration patterns and selection scenarios},
	url = {https://doi.org/10.1016/j.tpb.2014.10.006},
