	la = {en},
	pages = {1--11},
	publisher = {Elsevier BV},
	title = {Unstable population dynamics in obligate co-operators},
	url = {https://doi.org/10.1016/j.tpb.2020.09.002},
	volume = {136},
	year = {2020},
	bdsk-url-1 = {https://doi.org/10.1016/j.tpb.2020.09.002}}

