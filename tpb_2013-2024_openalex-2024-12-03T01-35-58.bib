	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2014.11.001},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {International HapMap Project; Haplotype Mapping; Marker-Assisted Selection; Genetic Value Prediction; Genetic Mapping},
	la = {en},
	pages = {18--30},
