	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Robustness (evolution); Ecosystem Resilience; Population Dynamics; Climate Tipping Points},
	la = {en},
	pages = {45--59},
	publisher = {Elsevier BV},
	title = {Ecological change points: The strength of density dependence and the loss of history},
