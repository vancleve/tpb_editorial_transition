	doi = {10.1016/j.tpb.2024.06.007},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Assortative mating; Divergence (linguistics); Group Selection; Information theory},
	la = {en},
	pages = {206--215},
	publisher = {Elsevier BV},
	title = {Unifying quantification methods for sexual selection and assortative mating using information theory.},
	url = {https://doi.org/10.1016/j.tpb.2024.06.007},
