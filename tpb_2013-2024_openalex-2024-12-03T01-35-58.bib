	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Lottery; Replicator equation; Density dependence; Population Dynamics; Frequency-dependent selection; Evolutionarily stable strategy},
	la = {en},
	pages = {82--90},
	publisher = {Elsevier BV},
	title = {The nest site lottery: How selectively neutral density dependent growth suppression induces frequency dependent selection},
	url = {https://doi.org/10.1016/j.tpb.2013.09.011},
