	doi = {10.1016/j.tpb.2018.09.005},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Habitat Fragmentation},
	la = {en},
	pages = {81--92},
	publisher = {Elsevier BV},
	title = {The Wright–Fisher site frequency spectrum as a perturbation of the coalescent’s},
	url = {https://doi.org/10.1016/j.tpb.2018.09.005},
	volume = {124},
