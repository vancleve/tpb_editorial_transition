	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2024.06.004},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Coalescent theory; Genetic Structure},
	la = {en},
	pages = {150--169},
	publisher = {Elsevier BV},
	title = {The coalescent in finite populations with arbitrary, fixed structure},
