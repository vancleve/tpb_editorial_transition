	la = {en},
	pages = {1--14},
	publisher = {Elsevier BV},
	title = {Cultural niche construction with application to fertility control: A model for education and social transmission of contraceptive use},
	url = {https://doi.org/10.1016/j.tpb.2023.06.001},
