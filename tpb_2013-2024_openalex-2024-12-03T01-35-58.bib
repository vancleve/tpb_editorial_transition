	journal = {Theoretical Population Biology},
	keywords = {Spatial heterogeneity; Community Ecology; Relative abundance distribution; Log-normal distribution},
	la = {en},
	pages = {65--74},
	publisher = {Elsevier BV},
	title = {Characteristics of temporal changes in communities where dynamics differ between species},
