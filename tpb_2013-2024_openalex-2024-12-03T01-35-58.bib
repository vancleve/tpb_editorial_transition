	journal = {Theoretical Population Biology},
	keywords = {Experimental Evolution; Population Genetics; Evolutionary Games; Genetic Dynamics; Spatial Patterns},
	la = {en},
	publisher = {Elsevier BV},
	title = {A stochastic field theory for the evolution of quantitative traits in finite populations},
