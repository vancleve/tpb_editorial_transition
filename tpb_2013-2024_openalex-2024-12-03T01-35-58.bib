	c1 = {Department of Human Genetics, University of Chicago, Chicago, IL, USA; Department of Biostatistics and Center for Statistical Genetics, University of Michigan, Ann Arbor, MI, USA; Department of Psychiatry, University of Michigan, Ann Arbor, MI, USA},
