	date-modified = {2024-12-02 20:58:00 -0500},
	doi = {10.1016/j.tpb.2021.09.004},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Birth–death process; Branching process; Gene Set Enrichment Analysis},
	la = {en},
	pages = {67--90},
	publisher = {Elsevier BV},
	title = {Exact site frequency spectra of neutrally evolving tumors: A transition between power laws reveals a signature of cell viability},
	url = {https://doi.org/10.1016/j.tpb.2021.09.004},
