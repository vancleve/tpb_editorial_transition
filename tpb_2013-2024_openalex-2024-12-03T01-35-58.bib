	doi = {10.1016/j.tpb.2016.05.005},
	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Evolutionary dynamics; Social dilemma; Social dynamics; Economic Experiments; Social evolution; Dynamics (music); Affect (linguistics); Evolutionary game theory},
