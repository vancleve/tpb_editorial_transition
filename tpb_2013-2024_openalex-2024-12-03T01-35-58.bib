	isbn = {0040-5809},
	journal = {Theoretical Population Biology},
	keywords = {Identity by descent; Genetic architecture; Genetic Value Prediction; Quantitative Genetics; Haplotype Mapping; Trait},
	la = {en},
	pages = {57--67},
	publisher = {Elsevier BV},
	title = {Correlation between relatives given complete genotypes: From identity by descent to identity by function},
