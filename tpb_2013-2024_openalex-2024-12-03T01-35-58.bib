	c1 = {Department of Biology, Stanford University, Stanford, CA, USA; Department of Evolutionary Anthropology, Duke University, Durham, NC, USA; Department of Systems Immunology \& Braunschweig Integrated Centre of Systems Biology, Helmholtz Centre for Infection Research, Braunschweig, Germany; Department of Biology, Stanford University, Stanford, CA, USA},
	date = {2020-04-07},
