	journal = {Theoretical Population Biology},
	keywords = {Experimental Evolution; Evolution; Host-Parasite Coevolution},
	la = {en},
	pages = {102--117},
	publisher = {Elsevier BV},
	title = {Limits to evolutionary rescue by conjugative plasmids},
	url = {https://doi.org/10.1016/j.tpb.2023.10.001},
